<Window x:Class="FFA_Clustering.TestResultsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="TestResults" Height="200" Width="554">
    <Grid>
        <ListView x:Name="ListViewInfoTestResults" Margin="10,10,10,10" IsHitTestVisible="True" BorderThickness="1" Background="White">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Algorithm" DisplayMemberBinding="{Binding Algorithm}" Width="200"/>
                    <GridViewColumn Header="SSE" DisplayMemberBinding="{Binding SumOfSquaredError}" Width="100"/>
                    <GridViewColumn Header="SM" DisplayMemberBinding="{Binding SilhouetteMethod}" Width="100"/>
                    <GridViewColumn Header="XB" DisplayMemberBinding="{Binding XieBeniIndex}" Width="100"/>
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>
